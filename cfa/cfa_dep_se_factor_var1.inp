TITLE:
	
	CFA Two Factors;
	Self-esteem and depression;

DATA:

	FILE IS se_dep_items.csv;

VARIABLE:
	
	Names are sad blues depress lonely crying tired failure
			  worthy capable goodqual attitude;
			  
MODEL:

	DEPRESSION BY sad*
				  blues-failure;
	SE BY worthy*
		 capable-attitude;
		 
	DEPRESSION@1;
	SE@1;
		 
