TITLE: 
	
	Mediation Model
	Model 1b
	Using MODEL INDIRECT COMMAND

DATA:

	FILE IS read_data.csv;

VARIABLE:
	
	NAMES ARE att4 math7 read7 read21 
			  vocab4 adopted male momed;
	USEVARIABLES ARE att4 read7 read21;
	MISSING ARE ALL (-9999);
	
MODEL:
	
	read21 ON read7 (a) 
			  att4 (cprime);
	read7 ON att4 (b);
	
MODEL INDIRECT:

	read21 IND att4;
	
MODEL CONSTRAINT:

	NEW (indirect total);
	indirect = a*b;
	total = (a*b) + cprime;
	
OUTPUT:

	STANDARDIZED (STDYX);
	CINTERVAL;
	
	
	
	
